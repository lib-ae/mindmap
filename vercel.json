{
  "version": 2,
  "builds": [
    {
      "src": "mindmap/app.py",
      "use": "@vercel/python",
      "config": { "maxLambdaSize": "15mb", "runtime": "python3.9" }
    }
  ],
  "routes": [
    {
      "src": "/static/(.*)",
      "dest": "/mindmap/static/$1"
    },
    {
      "src": "/(.*)",
      "dest": "mindmap/app.py"
    }
  ]
} 